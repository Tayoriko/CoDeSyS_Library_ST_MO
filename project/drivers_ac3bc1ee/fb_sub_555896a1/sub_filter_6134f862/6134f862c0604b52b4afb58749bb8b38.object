{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)6134f862-c060-4b52-b4af-b58749bb8b38","ParentGuid":"(Guid)00000000-0000-0000-0000-000000000000","Name":"(string)sub_Filter","Properties":{"@Type":"2c41fa04:IDictionary","@Value":[{"@Key":"(Guid)829a18f2-c514-4f6e-9634-1df173429203","@Value":{"@Type":"829a18f2","@Value":{"ParentObjects":{"@Type":"fa2ee218:IDictionary","@Value":[{"@Key":"(Guid)21af5390-2942-461a-bf89-951aaf6999f1","@Value":"(Guid)555896a1-6446-485f-a521-8b69565e7fd4"}]}}}}]},"TypeGuid":"(Guid)6f9dac99-8de1-4efc-8465-68ac443b7d08","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","829a18f2":"{829a18f2-c514-4f6e-9634-1df173429203}","fa2ee218":"{fa2ee218-a39b-4b6d-b249-49dbddbd168a}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"6f9dac99","@Value":{"SpecialFunc":{"@Type":"0db3d7bb:Enum","@Value":"None"},"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)//limit\n\tIF Filter_Lim > 100 THEN\n\t\tFilter_Lim := 100;\n\tEND_IF\n\n//adding new entyty\nIF Start AND (Filter_Type = 0 OR (Filter_Type = 1 AND sys.pulse_01s) OR (Filter_Type = 2 AND sys.pulse_1s)) THEN\n    IF cnt < Filter_Lim THEN\n        cnt := cnt + 1;\n    END_IF;\n\texe := TRUE;\nELSIF NOT Start THEN\n\tcnt := 0;\n\tResult := Signal;\nEND_IF;\n\n//start cycle\nIF exe THEN\n\t//read new value\n\tDATA[0] := Signal;\n\tSumm := 0.0;\n\t\n\t//limit values\n\tIF cnt > Filter_Lim THEN\n\t\tcnt := Filter_Lim;\n\tEND_IF;\n\t\n\t//shifting array\n\tFOR x := Filter_Lim TO 1 BY -1 DO\n\t\tDATA[x] := DATA[x - 1];\n\t\tIF x <= cnt AND Start THEN\n\t\t\tSumm := Summ + DATA[x];\n\t\tEND_IF;\n\tEND_FOR;\n\t\n\t//calculate median values\n\tIF cnt > 0 THEN\n\t\tResult := Summ / cnt;\n\tELSE\n\t\tResult := 0.0;\n\tEND_IF;\n\t\n\t//end cycle\n\texe := FALSE;\nEND_IF;\n","LineInfoPersistence":"(string)6134f862-c060-4b52-b4af-b58749bb8b38_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)FUNCTION_BLOCK sub_Filter\nVAR_INPUT\n\tStart\t\t\t:BOOL;\t\t//Enable filter\n\tSignal\t\t\t:REAL;\t\t//Input value\n\tFilter_Type\t\t:INT;\t\t//Execution clock for filter: 0 = every cycle of PLC, 1 = 0.1s, 2 = 1s\n\tFilter_Lim \t\t:INT;\t\t//Value of records in filter buffer\nEND_VAR\nVAR_OUTPUT\n\tResult\t\t\t:REAL;\t\t//Median value for all measurments\nEND_VAR\nVAR\n\tcnt\t\t\t\t:INT;\t\t//calculate actual measurments\n\tx\t\t\t\t:INT;\t\t//internal counter for shifting array elements\n\texe\t\t\t\t:BOOL;\t\t//command to execution filtering cycle\n\tSumm\t\t\t:REAL;\t\t//Summary value of all array's values\n\tDATA\t\t\t:ARRAY [0..100] OF REAL;\nEND_VAR\n","LineInfoPersistence":"(string)6134f862-c060-4b52-b4af-b58749bb8b38_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)81","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}