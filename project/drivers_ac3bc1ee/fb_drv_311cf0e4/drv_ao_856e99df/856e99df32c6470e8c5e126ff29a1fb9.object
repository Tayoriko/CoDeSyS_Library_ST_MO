{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)856e99df-32c6-470e-8c5e-126ff29a1fb9","ParentGuid":"(Guid)00000000-0000-0000-0000-000000000000","Name":"(string)drv_AO","Properties":{"@Type":"2c41fa04:IDictionary","@Value":[{"@Key":"(Guid)829a18f2-c514-4f6e-9634-1df173429203","@Value":{"@Type":"829a18f2","@Value":{"ParentObjects":{"@Type":"fa2ee218:IDictionary","@Value":[{"@Key":"(Guid)21af5390-2942-461a-bf89-951aaf6999f1","@Value":"(Guid)311cf0e4-1406-4bce-b577-8e3b742501ef"}]}}}}]},"TypeGuid":"(Guid)6f9dac99-8de1-4efc-8465-68ac443b7d08","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","829a18f2":"{829a18f2-c514-4f6e-9634-1df173429203}","fa2ee218":"{fa2ee218-a39b-4b6d-b249-49dbddbd168a}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"6f9dac99","@Value":{"SpecialFunc":{"@Type":"0db3d7bb:Enum","@Value":"None"},"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)//ANALOG OUTPUT\n//control of typical analog command\n// - Hz setpoint for motors\n// - % setpoint for valves\n\n//global commands\n\t//auto mode\n\t\tIF sys.All_Auto THEN\n\t\t\tCMD.plc.Manual := FALSE;\n\t\tEND_IF;\n\t//global reset\n\t\tIF sys.All_Reset THEN\n\t\t\tCMD.plc.Reset := TRUE;\n\t\tEND_IF\n\t\t\n//convert data\n\tIF cfg.cfg.plc.use_NoEmrg THEN\n\t\tsupport.0 := FALSE;\n\tELSE\n\t\tsupport.0 := sys.All_Block;\n\tEND_IF\n\t\n//device control\n\tdrv_CMD(Emergency := support.0,\n\t\t\tCMD := CMD);\n\t//manual mode flag\n\t\tIF CMD.plc.Manual THEN sys.man.plc.xAO := TRUE; END_IF;\n\t\tIF NOT CMD.plc.Manual THEN\n\t\t\tcfg.SP_M := Signal;\n\t\tEND_IF\n\t//disable\n\t\tIF NOT CMD.plc.Start_M THEN\n\t\t\tcfg.SP_M := 0;\n\t\tEND_IF\n\n//data convert to phisycal value scale\n\tdrv_Converter(\n\t\tSignal := cfg.SP_M,\n\t\tRange := cfg.Range,\n\t\tResult => Value);\n\t\n//convert data\n\tdrv_Scale(\n\t\tValue := Value,\n\t\tInput_Type := cfg.Output_Type,\n\t\tRange := cfg.Range,\n\t\tResult => state.Signal);\n\n//state signal transfer\n\tstate.Result := Value;\n\t\n//result\nIF NOT cfg.cfg.plc.use_Sim THEN\n\tResult_R := Value;\n\tResult_I := REAL_TO_INT(Value);\nELSE\n\tResult_R := 0.0;\n\tResult_I := 0;\nEND_IF\n\t\n//global state\n\tIF CMD.plc.Start_A THEN\n\t\tstate.state.plc.xON := TRUE;\n\t\tstate.state.plc.xOFF := FALSE;\n\tELSE\n\t\tstate.state.plc.xON := FALSE;\n\t\tstate.state.plc.xOFF := TRUE;\n\tEND_IF\n\t\n//HMI light\n\tIF cfg.cfg.plc.use_Light THEN\n\t\tIF CMD.plc.Start_A THEN state.Status := 1; END_IF\n\t\tIF NOT CMD.plc.Start_A THEN state.Status := 0; END_IF\n\tELSE\n\t\tstate.Status := 0;\n\tEND_IF\n\t\n//reset\n\tIF CMD.plc.Reset THEN\n\t\tstate.state.plc.Alarm := FALSE;\n\t\tCMD.plc.Alarm := FALSE;\n\t\tCMD.plc.Reset := FALSE;\n\tEND_IF\n\t","LineInfoPersistence":"(string)856e99df-32c6-470e-8c5e-126ff29a1fb9_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)FUNCTION_BLOCK drv_AO\nVAR_INPUT\n\tSignal\t\t\t:REAL;\t\t\t//Control signal from algorythm\nEND_VAR\t\nVAR_IN_OUT\n\tCMD\t\t\t\t:uni_CMD;\t\t//Non-retain structure with command word\n\tcfg\t\t\t\t:AO_cfg;\t\t//Retain structure with settings\n\tstate\t\t\t:AO_state;\t\t//Non-retain structure with states information\nEND_VAR\nVAR_OUTPUT\n\tResult_R\t\t:REAL;\t\t\t//Analog output signal to PLC in REAL\n\tResult_I\t\t:INT;\t\t\t//Analog output signal to PLC in INT\nEND_VAR\nVAR\n\tValue\t\t\t:REAL;\t\t\t//tmp value for calculating \n\tsupport\t\t\t:WORD;\t\t\t//internal command buffer\n\tcnt\t\t\t\t:INT;\t\t\t//internal counter\n\tPT\t\t\t\t:ARRAY [0..1] OF R_TRIG;\n\tNT\t\t\t\t:ARRAY [0..1] OF F_TRIG;\n\tFilter\t\t\t:sub_Filter;\nEND_VAR\n","LineInfoPersistence":"(string)856e99df-32c6-470e-8c5e-126ff29a1fb9_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)329","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}