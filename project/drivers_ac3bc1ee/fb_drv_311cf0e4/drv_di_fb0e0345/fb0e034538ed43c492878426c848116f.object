NOT Value THEN\n\t\t\tClock[1].CMD.plc.Start := FALSE;\n\t\t\tClock[0].CMD.plc.Start := TRUE;\n\t\t\tIF Clock[0].CMD.plc.Q THEN\n\t\t\t\tCMD.plc.Start_A := Signal;\n\t\t\tEND_IF;\n\t\tELSIF NOT Value THEN\n\t\t\tCMD.plc.Start_A := FALSE;\n\t\t\tClock[1].CMD.plc.Start := FALSE;\n\t\t\tClock[0].CMD.plc.Start := FALSE;\n\t\tEND_IF;\n\tELSE\n\t\tCMD.plc.Start_A := Value;\n\tEND_IF;\n\n//device control\n\tdrv_CMD(Emergency := FALSE,\n\t\tCMD := CMD);\n\t//manual mode flag\n\t\tIF CMD.plc.Manual THEN sys.man.plc.xDI := TRUE; END_IF;\n\n//result for main logic\n\tResult := CMD.plc.Start_M;\n\n//call timers\n\tClock[0].SP := INT_TO_REAL(cfg.SP_T_OFF);\n\tClock[1].SP := INT_TO_REAL(cfg.SP_T_ON);\n\tdrv_Time_R (Clock[0]);\n\tdrv_Time_R (Clock[1]);","LineInfoPersistence":"(string)fb0e0345-38ed-43c4-9287-8426c848116f_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)FUNCTION_BLOCK drv_DI\nVAR_INPUT\n\tSignal\t\t\t:BOOL;\t\t//Physical input value, i.e. %IX0.6\nEND_VAR\nVAR_IN_OUT\n\tCMD\t\t\t\t:uni_CMD;\t//Non-retain structure with command word\n\tcfg\t\t\t\t:DI_cfg;\t//Retain structure with settings\nEND_VAR\nVAR_OUTPUT\n\tResult\t\t\t:BOOL;\t\t//Logical output value, used in algorythms\nEND_VAR\nVAR\n\tValue\t\t\t:BOOL;\t\t//internal signal\n\tClock\t\t\t:ARRAY [0..1] OF Time_R;\nEND_VAR\n","LineInfoPersistence":"(string)fb0e0345-38ed-43c4-9287-8426c848116f_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)239","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.