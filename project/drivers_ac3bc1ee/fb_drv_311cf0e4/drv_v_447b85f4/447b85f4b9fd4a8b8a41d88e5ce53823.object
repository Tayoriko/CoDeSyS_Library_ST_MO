{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)447b85f4-b9fd-4a8b-8a41-d88e5ce53823","ParentGuid":"(Guid)00000000-0000-0000-0000-000000000000","Name":"(string)drv_V","Properties":{"@Type":"2c41fa04:IDictionary","@Value":[{"@Key":"(Guid)829a18f2-c514-4f6e-9634-1df173429203","@Value":{"@Type":"829a18f2","@Value":{"ParentObjects":{"@Type":"fa2ee218:IDictionary","@Value":[{"@Key":"(Guid)21af5390-2942-461a-bf89-951aaf6999f1","@Value":"(Guid)311cf0e4-1406-4bce-b577-8e3b742501ef"}]}}}}]},"TypeGuid":"(Guid)6f9dac99-8de1-4efc-8465-68ac443b7d08","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94","(Guid)3b83b776-fb25-43b8-99f2-3c507c9143fc"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","829a18f2":"{829a18f2-c514-4f6e-9634-1df173429203}","fa2ee218":"{fa2ee218-a39b-4b6d-b249-49dbddbd168a}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"6f9dac99","@Value":{"SpecialFunc":{"@Type":"0db3d7bb:Enum","@Value":"None"},"Implementation":{"@Type":"3b83b776","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)//VALVES\n//control of typical air and electrical valves\n// - doors\n// - dampers\n// - valves\n// - etc\n\n//global commands\n\t//start device\n\t\tIF cfg.cfg.plc.use_Signal THEN\n\t\t\tCMD.plc.Start_A := Signal;\n\t\tEND_IF\n\t//auto mode\n\t\tIF sys.All_Auto THEN\n\t\t\tCMD.plc.Manual := FALSE;\n\t\t\tCMD.plc.Force := FALSE;\n\t\tEND_IF;\n\t//simulation mode\n\t\tPT[0](CLK := sys.All_Sim);\n\t\tNT[0](CLK := sys.All_Sim);\n\t\tIF PT[0].Q THEN cfg.cfg.plc.use_Sim := TRUE; END_IF;\n\t\tIF NT[0].Q THEN cfg.cfg.plc.use_Sim := FALSE; END_IF;\n\t\tIF cfg.cfg.plc.use_Sim THEN sys.sim.plc.xV := TRUE; END_IF;\n\t//global reset\n\t\tIF sys.All_Reset THEN\n\t\t\tCMD.plc.Reset := TRUE;\n\t\tEND_IF\n\n//convert data\n\tIF cfg.cfg.plc.use_NoEmrg THEN\n\t\tsupport.0 := FALSE;\n\tELSE\n\t\tsupport.0 := sys.All_Block;\n\t\tIF support.0 THEN\n\t\t\tstate.Alarm.plc.alm_ES := TRUE;\n\t\t\tstate.Status := 99;\n\t\tEND_IF\n\tEND_IF\n\n//alarms without time control\n\t//QF control\n\t\tIF cfg.cfg.plc.use_QF THEN\n\t\t\tIF NOT FB_QF THEN\n\t\t\t\tstate.Alarm.plc.alm_QF := TRUE;\n\t\t\t\tstate.Status := 5;\n\t\t\tEND_IF\n\t\tEND_IF\n\t\n//device control\n\tdrv_CMD(Emergency := support.0,\n\t\tCMD := CMD);\n\t//manual mode flag\n\t\tIF CMD.plc.Manual THEN sys.man.plc.xV := TRUE; END_IF;\n\t\t\n//delay turn on\n\tIF cfg.SP_T_Delay_ON > 0 AND NOT CMD.plc.Manual THEN\n\t\tIF CMD.plc.Start_M THEN\n\t\t\tClock[0].CMD.plc.Start := TRUE;\n\t\t\tClock[1].CMD.plc.Start := FALSE;\n\t\t\tIF Clock[0].CMD.plc.Q THEN\n\t\t\t\tsupport.1 := TRUE;\n\t\t\tEND_IF\n\t\tELSE\n\t\t\tClock[0].CMD.plc.Start := FALSE;\n\t\tEND_IF\n\tELSE\n\t\tsupport.1 := CMD.plc.Start_M;\n\tEND_IF\n\n//delay turn off\n\tIF cfg.SP_T_Delay_OFF > 0 AND NOT CMD.plc.Manual THEN\n\t\tIF NOT CMD.plc.Start_M THEN\n\t\t\tClock[1].CMD.plc.Start := TRUE;\n\t\t\tClock[0].CMD.plc.Start := FALSE;\n\t\t\tIF Clock[1].CMD.plc.Q THEN\n\t\t\t\tsupport.1 := FALSE;\n\t\t\tEND_IF\n\t\tELSE\n\t\t\tClock[1].CMD.plc.Start := FALSE;\n\t\tEND_IF\n\tELSE\n\t\tsupport.1 := CMD.plc.Start_M;\n\tEND_IF\n\t\n//Opening\n\tIF support.1 AND state.Status < 4 AND NOT cfg.cfg.plc.use_Sim THEN\n\t\tIF cfg.cfg.plc.use_Air THEN\n\t\t\tsupport.2 := TRUE;\n\t\tELSIF NOT cfg.cfg.plc.use_Air AND (NOT FB_Open OR NOT cfg.cfg.plc.use_FB_Open) THEN\n\t\t\tsupport.2 := TRUE;\n\t\tELSE\n\t\t\tsupport.2 := FALSE;\n\t\tEND_IF\n\t\tsupport.3 := FALSE;\n\t\tClock[2].CMD.plc.Start := TRUE;\n\t\tClock[3].CMD.plc.Start := FALSE;\n\t\tIF cfg.cfg.plc.use_FB_Open THEN\n\t\t\t//opening in progress\n\t\t\tIF NOT Clock[2].CMD.plc.Q AND NOT FB_Open THEN\n\t\t\t\tstate.Status := 2;\n\t\t\tEND_IF\n\t\t\t//opening in fault\n\t\t\tIF Clock[2].CMD.plc.Q AND NOT FB_Open AND NOT state.state.plc.Pos_Ok THEN\n\t\t\t\tstate.Status := 6;\n\t\t\tEND_IF\n\t\t\t//opening complieted\n\t\t\tIF FB_Open THEN\n\t\t\t\tstate.Status := 1;\n\t\t\tEND_IF\n\t\tELSE\n\t\t\t//succesfuly\n\t\t\tstate.Status := 1;\n\t\tEND_IF\n\tEND_IF\n\t\n//Closing\n\tIF NOT support.1 AND NOT cfg.cfg.plc.use_Sim THEN\n\t\tsupport.2 := FALSE;\n\t\tIF cfg.cfg.plc.use_Air THEN\n\t\t\tsupport.3 := TRUE;\n\t\tELSIF NOT cfg.cfg.plc.use_Air AND (NOT FB_Close OR NOT cfg.cfg.plc.use_FB_Close) THEN\n\t\t\tsupport.3 := TRUE;\n\t\tELSE\n\t\t\tsupport.3 := FALSE;\n\t\tEND_IF\n\t\tClock[2].CMD.plc.Start := FALSE;\n\t\tClock[3].CMD.plc.Start := TRUE;\n\t\tIF cfg.cfg.plc.use_FB_Close THEN\n\t\t\t//opening in progress\n\t\t\tIF NOT Clock[3].CMD.plc.Q AND NOT FB_Close THEN\n\t\t\t\tstate.Status := 3;\n\t\t\tEND_IF\n\t\t\t//opening in fault\n\t\t\tIF Clock[3].CMD.plc.Q AND NOT FB_Close AND NOT state.state.plc.Pos_Ok THEN\n\t\t\t\tstate.Status := 7;\n\t\t\tEND_IF\n\t\t\t//opening complieted\n\t\t\tIF FB_Close AND state.Status < 4 THEN\n\t\t\t\tstate.Status := 0;\n\t\t\tEND_IF\n\t\tELSE\n\t\t\t//succesfuly\n\t\t\tstate.Status := 0;\n\t\tEND_IF\n\tEND_IF\n\t\n\t\t\n//alarms with time control\n\t//open alarm control\n\t\tdrv_Alarm(\n\t\t\tSignal := (Clock[2].CMD.plc.Q AND NOT FB_Open AND NOT state.state.plc.Pos_Ok),\n\t\t\tReset := (CMD.plc.Reset OR cfg.cfg.plc.use_Sim),\n\t\t\tClock := Clock[2].CMD,\n\t\t\tAlarm := alm[0]);\n\t\tstate.Alarm.plc.alm_Open := alm[0];\n\t//close alarm control\n\t\tdrv_Alarm(\n\t\t\tSignal := (Clock[3].CMD.plc.Q AND NOT FB_Close AND NOT state.state.plc.Pos_Ok),\n\t\t\tReset := (CMD.plc.Reset OR cfg.cfg.plc.use_Sim),\n\t\t\tClock := Clock[3].CMD,\n\t\t\tAlarm := alm[1]);\n\t\tstate.Alarm.plc.alm_Close := alm[1];\t\n\n//alarm control\n\tIF (state.Status > 4 OR state.Alarm.hmi > 0) AND NOT cfg.cfg.plc.use_Sim THEN\n\t\tstate.state.plc.Alarm := TRUE;\n\t\tsys.alm.plc.xV := TRUE;\n\t\tCMD.plc.Alarm := TRUE;\n\tEND_IF\t\n\t\n//disable close while alarm\n\tIF NOT cfg.cfg.plc.use_CWF AND state.state.plc.Alarm THEN\n\t\tsupport.2 := FALSE;\n\t\tsupport.3 := FALSE;\n\tEND_IF\n\t\n//reset\n\tIF CMD.plc.Reset THEN\n\t\tIF state.Status > 4 THEN\n\t\t\tstate.Status := 0;\n\t\tEND_IF\n\t\tFOR cnt := 0 TO 6 DO\n\t\t\tClock[cnt].CMD.plc.Reset := TRUE;\n\t\tEND_FOR\n\t\tstate.state.plc.Alarm := FALSE;\n\t\tstate.Alarm.hmi := 0;\n\t\tCMD.plc.Alarm := FALSE;\n\t\tCMD.plc.Reset := FALSE;\n\tEND_IF\n\n//Simulation\n\tIF cfg.cfg.plc.use_Sim THEN\n\t\tIF support.1 THEN\n\t\t\tstate.Status := 1;\n\t\tELSE\n\t\t\tstate.Status := 0;\n\t\tEND_IF\n\tEND_IF\n\t\n//ON & OFF, state\n\tIF (FB_Open OR (CMD.plc.Start_M AND (cfg.cfg.plc.use_Sim OR NOT cfg.cfg.plc.use_FB_Open))) AND state.Status < 4 THEN\n\t\tstate.state.plc.xON := TRUE;\t\n\t\tstate.state.plc.xOFF := FALSE;\t\n\tELSIF (FB_Close OR (NOT CMD.plc.Start_M AND (cfg.cfg.plc.use_Sim OR NOT cfg.cfg.plc.use_FB_Close)))AND state.Status < 4 THEN\n\t\tstate.state.plc.xON := FALSE;\t\n\t\tstate.state.plc.xOFF := TRUE;\n\tELSE\n\t\tstate.state.plc.xON := FALSE;\t\n\t\tstate.state.plc.xOFF := FALSE;\n\tEND_IF\n\t\n//Done\n\tDone := state.state.plc.xON;\n\n//position control for once moving\n\tPT[1] (CLK := CMD.plc.Start_M);\n\tNT[1] (CLK := CMD.plc.Start_M);\n\tPT[2] (CLK := FB_Open); \n\tPT[3] (CLK := FB_Close);\n\tIF cfg.cfg.plc.use_Once THEN\n\t\tIF PT[2].Q OR PT[3].Q THEN\n\t\t\tstate.state.plc.Pos_Ok := TRUE;\n\t\tEND_IF\n\t\tIF PT[1].Q OR NT[1].Q THEN\n\t\t\tstate.state.plc.Pos_Ok := FALSE;\n\t\tEND_IF\n\tELSE\n\t\tstate.state.plc.Pos_Ok := FALSE;\n\tEND_IF\n\t\n//commands\n\t//turn on\n\t\tIF support.2 AND NOT state.state.plc.Pos_Ok THEN\n\t\t\tCMD_Open := TRUE;\n\t\t\tCMD_Close := FALSE;\n\t\tEND_IF\n\t//turn off\n\t\tIF support.3 AND NOT state.state.plc.Pos_Ok THEN\n\t\t\tIF NOT cfg.cfg.plc.use_Duo THEN\n\t\t\t\tCMD_Open := FALSE;\n\t\t\t\tCMD_Close := TRUE;\n\t\t\tELSE\n\t\t\t\tCMD_Open := TRUE;\n\t\t\t\tCMD_Close := TRUE;\n\t\t\tEND_IF\n\t\tEND_IF\n\t//shutdown\n\t\tIF (NOT support.2 AND NOT support.3) OR state.state.plc.Pos_Ok OR cfg.cfg.plc.use_Sim THEN\n\t\t\tCMD_Open := FALSE;\n\t\t\tCMD_Close := FALSE;\n\t\tEND_IF\n\t//low speed\n\t\tIF CMD_Open OR CMD_Close THEN\n\t\t\tIF CMD.plc.LowSpeed THEN\n\t\t\t\tCMD_Low := TRUE;\n\t\t\tELSE\n\t\t\t\tCMD_Low := FALSE;\n\t\t\tEND_IF\n\t\tEND_IF\n\t\n//call timers\n\tClock[0].SP := cfg.SP_T_Delay_ON;\n\tClock[1].SP := cfg.SP_T_Delay_OFF;\n\tClock[2].SP := cfg.SP_T_ON;\n\tClock[3].SP := cfg.SP_T_OFF;\n\tFOR cnt := 0 TO 3 DO\n\t\tdrv_Time_S (Clock[cnt]);\n\tEND_FOR","LineInfoPersistence":"(string)447b85f4-b9fd-4a8b-8a41-d88e5ce53823_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)FUNCTION_BLOCK drv_V\nVAR_INPUT\n\tSignal\t\t\t:BOOL;\t\t\t//Command in automation mode from algorythms\n\tFB_QF\t\t\t:BOOL;\t\t\t//Physical input value, control feedback signal 'Ready' or NOT 'Alarm'\n\tFB_Open\t\t\t:BOOL;\t\t\t//Physical input value, control feedback signal 'Open'\n\tFB_Close\t\t:BOOL;\t\t\t//Physical input value, control feedback signal 'Close'\nEND_VAR\nVAR_IN_OUT\n\tCMD\t\t\t\t:uni_CMD;\t\t//Non-retain structure with command word\n\tcfg\t\t\t\t:V_cfg;\t\t\t//Retain structure with settings\n\tstate\t\t\t:V_state;\t\t//Non-retain structure with states information\nEND_VAR\nVAR_OUTPUT\n\tCMD_Open\t\t:BOOL;\t\t\t//Phisical output signal for opening, i.e. %QX2.4\n\tCMD_Close\t\t:BOOL;\t\t\t//Phisical output signal for closing, i.e. %QX2.5\n\tCMD_Low\t\t\t:BOOL;\t\t\t//Phisical output signal for speed control, i.e. %QX2.7 - optional\n\tDone\t\t\t:BOOL;\t\t\t//Device is open\nEND_VAR\nVAR\n\tsupport\t\t\t:WORD;\n\tcnt\t\t\t\t:INT;\t\t\t//internal counter\n\talm\t\t\t\t:ARRAY [0..3] OF BOOL;\t\t\t//support word for transfer alarms\n\tPT\t\t\t\t:ARRAY [0..3] OF R_TRIG;\n\tNT\t\t\t\t:ARRAY [0..1] OF F_TRIG;\n\tClock\t\t\t:ARRAY [0..3] OF Time_S;\nEND_VAR","LineInfoPersistence":"(string)447b85f4-b9fd-4a8b-8a41-d88e5ce53823_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)327","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"0db3d7bb":"{0db3d7bb-cde0-4416-9a7b-ce49a0124323}","3b83b776":"{3b83b776-fb25-43b8-99f2-3c507c9143fc}","6f9dac99":"{6f9dac99-8de1-4efc-8465-68ac443b7d08}","8e575c5b":"{8e575c5b-1d37-49c6-941b-5c0ec7874787}","a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","ArrayList":"System.Collections.ArrayList","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","string":"System.String"}}},"FormatVersion":"1.0"}