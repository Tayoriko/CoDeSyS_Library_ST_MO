ge.Min_Out;\n    Value_Max := Range.Max_Out;\nEND_IF;\nIF Range.Min_Out <= 0 AND Range.Max_Out <= 0 THEN\n    Range_Out := ABS(Range.Max_Out) + ABS(Range.Min_Out);\n\tValue_Min := Range.Min_Out;\n    Value_Max := Range.Max_Out;\nEND_IF;\nIF Range.Min_Out >= 0 AND Range.Max_Out <= 0 THEN\n    Range_Out := ABS(Range.Max_Out) + ABS(Range.Min_Out);\n\tValue_Min := Range.Max_Out;\n    Value_Max := Range.Min_Out;\nEND_IF;\n\n//Соотношение диапазонов\nIF Range_Out <> 0 THEN\n\tRR_Scale := Range_In / Range_Out;\nEND_IF\n\n//Линеаризация\nIF RR_Scale <> 0 THEN\n\tBuffer := Signal / RR_Scale;\nEND_IF\n\n//Коррекция\nBuffer := Buffer + Value_Min;\n\n//Инверсия\nIF inv THEN\n    Buffer := Value_Max - Buffer;\nEND_IF;\n\n//Ограничение снизу\nIF Buffer < Value_Min THEN\n    Buffer := Value_Min;\nEND_IF;\n\n//Ограничение сверху\nIF Buffer > Value_Max THEN\n    Buffer := Value_Max;\nEND_IF;\n\n//Вывод значения\nResult := Buffer;","LineInfoPersistence":"(string)41a1ba09-24dc-445d-a7b0-b2835b4ed5c3_Impl_LineIds"}}}},"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string)FUNCTION drv_Converter : bool\nVAR_INPUT\n\tSignal\t\t\t:REAL;\t\t\t//Analog input signal from PLC\n\tRange\t\t\t:list_Range;\t//Signal scale range\nEND_VAR\nVAR_OUTPUT\n\tResult\t\t\t:REAL;\t\t\t//Convertation result\nEND_VAR\nVAR\n\tinv\t\t\t\t:BOOL;\t\t\t//inverse range\n\tRange_In\t\t:REAL;\t\t\t//full input range\n\tRange_Out\t\t:REAL;\t\t\t//full value range\n\tRR_Scale\t\t:REAL;\t\t\t//range in to range out scale\n\tValue_Min\t\t:REAL;\t\t\t//actual minimum output value\n\tValue_Max\t\t:REAL;\t\t\t//actual maximum output value\n\tBuffer\t\t\t:REAL;\t\t\t//tmp value for calculating\nEND_VAR\n","LineInfoPersistence":"(string)41a1ba09-24dc-445d-a7b0-b2835b4ed5c3_Decl_LineIds"}}}},"UniqueIdGenerator":"(string)105","POULevel":{"@Type":"8e575c5b:Enum","@Value":"Standard"},"ChildObjectGuids":{"@Type":"ArrayList:IList","@Value":[]},"AddAttributeSubsequ