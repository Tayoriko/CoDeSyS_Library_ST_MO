{"payload":{"meta":{"Graph":{"@Type":"81297157","@Value":{"Guid":"(Guid)233e6cb1-f9f6-4651-a8e4-9a517068d8d6","ParentGuid":"(Guid)00000000-0000-0000-0000-000000000000","Name":"(string)sys","Properties":{"@Type":"2c41fa04:IDictionary","@Value":{}},"TypeGuid":"(Guid)ffbfa93a-b94d-45fc-a329-229860183b1d","EmbeddedTypeGuids":{"@Type":"[Guid]","@Value":["(Guid)a9ed5b7e-75c5-4651-af16-d2c27e98cb94"]}}},"TypeInfos":{"2c41fa04":"{2c41fa04-1834-41c1-816e-303c7aa2c05b}","81297157":"{81297157-7ec9-45ce-845e-84cab2b88ade}","Guid":"System.Guid","string":"System.String"}},"object":{"Graph":{"@Type":"ffbfa93a","@Value":{"Interface":{"@Type":"a9ed5b7e","@Value":{"TextDocument":{"@Type":"f3878285","@Value":{"TextBlobForSerialisation":"(string){attribute 'qualified_only'}\nVAR_GLOBAL\n\t//General management\n\t\t//Global controls\n\t\t\tAll_Block\t\tAT %MX0.0\t:BOOL;\t\t\t\t//Emergency stop\n\t\t\tAll_Reset\t\tAT %MX0.1\t:BOOL;\t\t\t\t//Reset all alarms\n\t\t\tAll_Sim\t\t\tAT %MX0.2\t:BOOL;\t\t\t\t//Switch simulation mode for all devices\n\t\t\tAll_Auto\t\tAT %MX0.3\t:BOOL;\t\t\t\t//Reset \"CMD.plc.Manual\" for all devices\n\t\t\tSilence\t\t\tAT %MX0.4\t:BOOL;\t\t\t\t//Disable siren from HMI\n\t\t\tSave\t\t\tAT %MX0.5\t:BOOL;\t\t\t\t//Save data to PVL\n\t\t//Service tags\n\t\t\talm\t\t\t\tAT %MW1\t\t:uni_dev_list;\t\t//Alarms flags for devices\n\t\t\tman\t\t\t\tAT %MW2\t\t:uni_dev_list;\t\t//Manual flags for devices\n\t\t\tsim\t\t\t\tAT %MW3\t\t:uni_dev_list;\t\t//Simulation flags for devices\n\t\t\tDTL\t\t\t\t:udt_DTL;\t\t\t\t\t\t//System time\n\t\t\tUpTime\t\t\t:TIME;\t\t\t\t\t\t\t//Work time after power on\n\t\t\tUpTime_ms\t\t:UDINT;\t\t\t\t\t\t\t//Work time after power on as millisecond\n\t\t//Global data\n\t\t\tLibVersion\t\tAT %ML1\t\t:VERSION;\t\t\t//Version code\n\t\t\n\t\t//Additional tags and constans\n\t\t\tsecond\t\t\t:REAL := (1.0/3600.0);\t//wight of 1 second in hour format\n\t\t\tdev\t\t\t\t:INT;\t\t\t\t\t//device counter for FOR\n\t\t\taws_ON\t\t\t:BOOL := TRUE;\t\t\t//always TRUE\n\t\t\taws_OFF\t\t\t:BOOL := FALSE;\t\t\t//always FALSE\n\t\t\tStartUp_Load\t:BOOL;\t\t//First load configuration after power on\n\t\t\t\t\n\t\t//Impulse tags\n\t\t\tpulse_01s\t:BOOL;\t\t\t\t\t//global trigger 100 ms\n\t\t\tpulse_1s\t:BOOL;\t\t\t\t\t//global trigger 1 second\n\t\t\t\n\t\t//Clock tags\n\t\t\tclock_01s\t:BOOL;\t\t\t\t\t//global clock for 100 ms\n\t\t\tclock_05s\t:BOOL;\t\t\t\t\t//global clock for 500 ms\n\t\t\tclock_1s\t:BOOL;\t\t\t\t\t//global clock for 1 s\nEND_VAR","LineInfoPersistence":"(string)233e6cb1-f9f6-4651-a8e4-9a517068d8d6_Decl_LineIds"}}}},"NetVarProperties":null,"ParameterList":"(bool)False","AddAttributeSubsequent":"(bool)False"}},"TypeInfos":{"a9ed5b7e":"{a9ed5b7e-75c5-4651-af16-d2c27e98cb94}","bool":"System.Boolean","f3878285":"{f3878285-8e4f-490b-bb1b-9acbb7eb04db}","ffbfa93a":"{ffbfa93a-b94d-45fc-a329-229860183b1d}","string":"System.String"}}},"FormatVersion":"1.0"}